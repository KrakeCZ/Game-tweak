Windows Registry Editor Version 5.00     



; –– Turn OFF Widgets, Chat, Copilot ––
[HKEY_LOCAL_MACHINE\SOFTWARE\Policies\Microsoft\Dsh]
"AllowNewsAndInterests"=dword:00000000

[HKEY_LOCAL_MACHINE\SOFTWARE\Policies\Microsoft\Windows\Windows Feeds]
"EnableFeeds"=dword:00000000



; –– Disable Superfetch / SysMain ––
[HKEY_LOCAL_MACHINE\SYSTEM\CurrentControlSet\Services\SysMain]
"Start"=dword:00000004


; –– Disable Prefetch ––
[HKEY_LOCAL_MACHINE\SYSTEM\CurrentControlSet\Control\Session Manager\Memory Management\PrefetchParameters]
"EnablePrefetcher"=dword:00000000
"EnableSuperfetch"=dword:00000000




; –– disable window animation  ––
[HKEY_CURRENT_USER\Control Panel\Desktop\WindowMetrics]
"MinAnimate"="0"

[HKEY_CURRENT_USER\Software\Policies\Microsoft\Windows\Explorer]
"NoWindowMinimizingShortcuts"=dword:00000001

[HKEY_CURRENT_USER\Control Panel\Desktop]
"ForegroundLockTimeout"="0"

; –– Disable  welcom splash screen (app) ––
[HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Windows\CurrentVersion\Policies\System]
"DisableBackButtonAppNav"=dword:00000001



==== Disable usb sleep===
[HKEY_LOCAL_MACHINE\SYSTEM\CurrentControlSet\Services\USB]
"DisableSelectiveSuspend"=dword:00000001


===disable win search if use from explorer must wait 1s===
[HKEY_LOCAL_MACHINE\SYSTEM\CurrentControlSet\Services\WSearch]
"Start"=dword:00000004



;======= CURSOR refresh (oem5 to 1 ) =======
[HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Input\Settings\ControllerProcessor\CursorSpeed]
"CursorUpdateInterval"=dword:00000001




 ============================================================================================

; ===== WIN32PRIORITYSEPARATION =====   24 / 26 / 28 / 2a  best for game and streaming (this tweak not need restart easy made reg file and try during game and check mouse/kb/gamepad latence in game )
 (more info https://www.xbitlabs.com/win32priorityseparation-performance/)

[HKEY_LOCAL_MACHINE\SYSTEM\CurrentControlSet\Control\PriorityControl]
"Win32PrioritySeparation"=dword:00000024


; =====Improves system responsiveness and network speed =====
[HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Windows NT\CurrentVersion\Multimedia\SystemProfile]
"SystemResponsiveness"=dword:00000000
"NetworkThrottlingIndex"=dword:ffffffff
"NoLazyMode"=dword:00000001 
 "AlwaysOn"=dword:00000001


; ===== Speeds up Shut Down time =====   set 150-1000   oem is 5000ms
[HKEY_LOCAL_MACHINE\SYSTEM\CurrentControlSet\Control]
"WaitToKillServiceTimeout"="150"     


; ===== Zero Startup Delay  =====
[HKEY_CURRENT_USER\Software\Microsoft\Windows\CurrentVersion\Explorer\Serialize]
"StartupDelayInMSec"=dword:00000000

; ===== Disable Windows App tracking to improve Start and Search Results =====
[HKEY_CURRENT_USER\Software\Policies\Microsoft\Windows\EdgeUI]
"DisableMFUTracking"=dword:00000001


; ===== Disables app suggestions on start ===== 
[HKEY_LOCAL_MACHINE\SOFTWARE\Policies\Microsoft\Windows\CloudContent]
"DisableWindowsConsumerFeatures"=dword:00000001


; ===== Ram speed ===== for 32gb-33554432  /64gb- 67108864  /16gb 16777216===
[HKEY_LOCAL_MACHINE\SYSTEM\ControlSet001\Control]
"SvcHostSplitThresholdInKB"=dword:33554432


; ===== Slightly improves RAM management and overall system speed =====
[HKEY_CURRENT_USER\Control Panel\Desktop]
"AutoEndTasks"="1"
"HungAppTimeout"="70"
"MenuShowDelay"="0"
"WaitToKillAppTimeout"="200"
"LowLevelHooksTimeout"="100"
  
===improve devices ===
[HKEY_LOCAL_MACHINE\SYSTEM\CurrentControlSet\Control\Session Manager\Memory Management]
"IoPageLockLimit"=dword:20000000
   

; ===== Turn off LargeSystemCache ===== 
[HKEY_LOCAL_MACHINE\SYSTEM\CurrentControlSet\Control\Session Manager\Memory Management]
"LargeSystemCache"=dword:00000000



